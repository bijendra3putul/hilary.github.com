layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
  <title><![CDATA[{{ site.title }}: {{ page.title }}]]></title>
  <link href="{{ site.url }}{{ page.feed_url }}" rel="self"/>
  <link href="{{ site.url }}"/>
  <updated>{{ page.updated | date_to_xmlschema }}</updated>
  {% include helpers/site-meta.xml %}

  {% for post in site.tags[page.tag] limit: 20 %}
    {% include articles/feed-entry.xml %}
  {% endfor %}
</feed>

